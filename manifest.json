{
  "manifest_version": 3,
  "name": "Volume Booster",
  "version": "1.6",
  "description": "Boost tab volume 1â€“20x in real-time",
  "permissions": ["activeTab", "storage", "scripting"],
  "icons": {
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
